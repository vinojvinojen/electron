  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^27.0.3",
    "electron-is-dev": "^2.0.0",
    "pkg": "^5.8.1",
    "wait-on": "^7.1.0"
  }
